# Git aliases

alias gst='git status'
alias gco='git checkout'
alias gcb='git checkout -b'
alias gb='git branch'
alias gac='git add -A && git commit -v'
alias guc='git add -u && git commit -v'
alias gp='git push'
alais gpl='git pull'
alias gf='git fetch'
alias gcp='git cherry-pick'
alias gcpno='git cherry-pick --no-commit'

